using System;
public class Pattern
{
public static void Main(string[] args)
{
  int num;
  Console.WriteLine("Enter the Number");
  num = Convert.ToInt32(Console.ReadLine());
  
    int i=num;
    for(int j=i;j>0;j--)
     Console.Write(i);
    Console.WriteLine();
    for(int j=num;j>0;j--)
     Console.Write(j);
 }
}
